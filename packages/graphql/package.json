{
  "name": "@solui/graphql",
  "version": "1.22.0",
  "description": "GraphQL utilities for solUI",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist && mkdir ./dist",
    "compile": "babel ./src --out-dir ./dist",
    "build": "npm run clean && npm run compile",
    "watch": "npm run clean && npm run compile -- --watch"
  },
  "keywords": [
    "graphql",
    "client",
    "solui",
    "ethereum",
    "solidity",
    "ui"
  ],
  "files": [
    "dist"
  ],
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solui/solui.git"
  },
  "bugs": {
    "url": "https://github.com/solui/solui/issues"
  },
  "homepage": "https://solui.dev",
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/node": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-proposal-export-default-from": "^7.8.0",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1"
  },
  "dependencies": {
    "@solui/utils": "^1.22.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "graphql": "^14.5.8",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "graphql-type-json": "^0.3.1",
    "node-fetch": "^2.6.0"
  }
}
