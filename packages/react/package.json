{
  "name": "@solui/react",
  "version": "1.7.1",
  "description": "React UI components for solUI",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist && mkdir ./dist",
    "compile": "babel ./src --out-dir ./dist",
    "build": "npm run clean && npm run compile",
    "watch": "npm run clean && npm run compile -- --watch"
  },
  "keywords": [
    "parser",
    "solui",
    "ethereum",
    "solidity",
    "ui"
  ],
  "files": [
    "dist"
  ],
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solui/solui.git"
  },
  "bugs": {
    "url": "https://github.com/solui/solui/issues"
  },
  "homepage": "https://solui.dev",
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/node": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-proposal-export-default-from": "^7.8.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "@emotion/styled": "^10.0.17",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@reach/observe-rect": "^1.0.3",
    "@solui/graphql": "^1.7.0",
    "@solui/styles": "^1.7.0",
    "@solui/utils": "^1.7.0",
    "clipboard-polyfill": "^2.8.6",
    "emotion-theming": "^10.0.27",
    "prop-types": "^15.7.2",
    "react-dom": "^16.12.0",
    "react-svg": "^10.0.20",
    "react-tippy": "^1.3.4",
    "react-use": "^12.9.1",
    "styled-components": "^4.4.1",
    "styled-react-modal": "^2.0.0",
    "uuid": "^3.3.3"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
