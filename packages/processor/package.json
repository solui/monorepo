{
	"name": "@solui/processor",
	"version": "1.22.0",
	"description": "Parser and processor for UI specifications",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"parser",
		"solui",
		"ethereum",
		"solidity",
		"ui"
	],
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rm -rf ./dist && mkdir ./dist",
		"compile": "babel ./src --out-dir ./dist",
		"build": "npm run clean && npm run compile",
		"watch": "npm run clean && npm run compile -- --watch"
	},
	"author": "Ramesh Nair <ram@hiddentao.com>",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "src/index.js",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/solui/solui.git"
	},
	"bugs": {
		"url": "https://github.com/solui/solui/issues"
	},
	"homepage": "https://solui.dev",
	"devDependencies": {
		"@babel/cli": "^7.8.0",
		"@babel/node": "^7.8.0",
		"@babel/plugin-proposal-class-properties": "^7.8.0",
		"@babel/plugin-proposal-export-default-from": "^7.8.0",
		"@babel/preset-env": "^7.6.0",
		"babel-eslint": "^10.0.3",
		"core-js": "^3.2.1",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"eslint-plugin-standard": "^4.0.1"
	},
	"dependencies": {
		"@solui/utils": "^1.22.0"
	}
}
